{
  "artifactory": {
    "url": "https://burnitoken.jfrog.io/artifactory",
    "user": "${JFROG_USER}",
    "password": "${JFROG_PASSWORD}"
  },
  "security": {
    "scanOnBuild": true,
    "failOnSeverity": "high",
    "licenses": {
      "allow": [
        "MIT",
        "Apache-2.0",
        "ISC"
      ],
      "deny": [
        "GPL-3.0"
      ]
    }
  }
}